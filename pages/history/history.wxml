<scroll-view class="scol" style="height: {{screenHeight}}px;" scroll-y>
<block wx:for="{{history}}">
  <view class="historycontain">
  <view style="background-image: url({{item.imageFrontUrl}});" class="img"/>
  <view class="button" bind:tap="toshare" data-index="{{item}}">查看</view>
  <view class="button1" bind:tap="tomake" data-index="{{item}}">修改</view>
  <view class="time">{{item.createTime}}</view>
  </view>
</block>
<view class="historycontain" style="height:15%;box-shadow: 0 0 0 0;">
  </view>
</scroll-view>